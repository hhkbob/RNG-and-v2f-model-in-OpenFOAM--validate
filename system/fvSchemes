FoamFile
{
	version	2.0;
	class	dictionary; 
	format	ascii;
	location	"system";
	object	fvSchemes;  
}
ddtSchemes 
{
	default	steadyState; 
}
gradSchemes
{
	default	Gauss linear;
}
divSchemes
{
	default	bounded Gauss upwind; 
	div(phi,U)	bounded Gauss linearUpwind grad(U); //QUICK is good
	div(phi,T)	bounded Gauss QUICK;
	div(phi,omega)	bounded Gauss QUICK;
	div(phi,epsilon)	bounded Gauss QUICK; //upwind is good
	div(phi,k)	bounded Gauss linearUpwind grad; //QUICK is good
	div((nuEff*dev(T(grad(U)))))	Gauss linear;
	div((nuEff*dev2(T(grad(U)))))	Gauss linear;
	div(R)	Gauss linear;
}
laplacianSchemes
{
	default	Gauss linear corrected;
}
interpolationSchemes
{
	default	linear;
}
snGradSchemes
{
	default	corrected;
}
fluxRequired
{
	default	no;
	p_rgh	;
}
wallDist
{
	method	meshWave;
	nRequired	true;
}
